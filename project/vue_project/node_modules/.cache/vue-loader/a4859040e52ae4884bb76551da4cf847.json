>\r\n            <span class=\"2\">\r\n                <a href=\"#\">忘记密码?</a>\r\n            </span>\r\n        </div>\r\n      </div>\r\n  </div>\r\n      \r\n</template>\r\n<script>\r\n  import {Toast} from \"mint-ui\";\r\n  export default {\r\n      data(){\r\n          return{\r\n              uname:\"\",\r\n              upwd:\"\"\r\n          }\r\n      },\r\n      methods:{\r\n        toSubmit(){\r\n            var obj={\r\n                uname:this.uname,\r\n                upwd:this.upwd\r\n            }\r\n            this.$http.post(\"user/login\",obj).then(result=>{\r\n                // console.log(result);\r\n                Toast(result.body.msg)\r\n                if(result.body.code!=1){\r\n                    return;\r\n                }else{\r\n                    this.$router.push({path:'/index'});\r\n                }\r\n            });\r\n        }\r\n      },\r\n      created(){\r\n         \r\n      },\r\n  }\r\n</script>\r\n<style>\r\n.app_login{\r\n    width:100%;\r\n    height:800px;\r\n    margin:50px auto;\r\n    background:url(../../img/register.png) no-repeat center center;\r\n    background-size: cover;\r\n    position: relative;\r\n}\r\n.app_login .content{\r\n    width:600px;\r\n    height:450px;\r\n    background:rgba(0,0,0,.2);\r\n    padding:50px;\r\n    position: absolute;\r\n    top:21%;\r\n    right:8%;\r\n    color:white ;\r\n}\r\n.app_login .content a,.app_login .content h5,.app_login .content button{\r\n    color:white\r\n}\r\n.app_login .content>div{\r\n    display: flex;\r\n    justify-content: space-around;\r\n    margin-top:30px;\r\n}\r\n.app_login .content .row_header h4,.app_login .content .row_header h5{\r\n    width:50%;\r\n    text-align: center;\r\n}\r\n.app_login .content .row_body h4{\r\n    width:20%;\r\n    padding:5px 0;\r\n}\r\n.app_login .content .row_body p{\r\n    width:50%;\r\n}\r\n.app_login .content .row_body h5{\r\n    width:30%;\r\n    padding:5px 15px;\r\n}\r\n.app_login .content .row_footer{\r\n 