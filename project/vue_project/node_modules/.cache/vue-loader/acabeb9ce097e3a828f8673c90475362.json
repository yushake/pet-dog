ect mui-pull-left\" src=\"../../img/shuijiao.jpg\">\r\n                    <div class=\"mui-media-body\">\r\n                        <h1>{{item.title}}</h1>\r\n                        <p class='mui-ellipsis'>\r\n                            <span> {{item.ctime | dateFormat}}</span>\r\n                            <span>点击:{{item.click}}次</span>\r\n                        </p>\r\n                    </div>\r\n                </router-link>\r\n            </li>\r\n        </ul>\r\n        <mt-button type=\"primary\" size=\"large\" @click=\"getMore\">加载更多...</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n    import {Toast} from \"mint-ui\";\r\n\r\n    export default {\r\n        data(){\r\n            return{\r\n                list:[],\r\n                pageIndex:0   //页码\r\n            }\r\n        },\r\n        methods:{\r\n            // getNewsList(){\r\n                // var url=\"newslist/list\";\r\n                // this.$http.get(url).then(result=>{\r\n                //     if(result.body.code==1){\r\n                //         this.list=result.body.msg.data;\r\n                //     }else{\r\n                //          Toast(\"获取列表失败\")\r\n                //     }\r\n                // })\r\n            // },\r\n            getMore(){\r\n                this.pageIndex++;\r\n                var url=\"newslist/list?pno=\"+this.pageIndex;\r\n                this.$http.get(url).then(result=>{\r\n                    // 赋值有问题,覆盖前一页数据\r\n                    // this.list=result.body.msg.data;\r\n                    // 保存前一页 concat\r\n                    if(this.pageIndex<result.body.msg.pageCount+1){\r\n                        console.log(result);\r\n                        this.list=this.list.concat(result.body.msg.data);\r\n                    }else{\r\n                        Toast(\"没有更多了\");\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        created(){\r\n            this.getMore();\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n.app_newslist .mui-table-view h1{\r\n    font-size:14px;\r\n}\r\n.app_newslist li .mui-ellipsis{\r\n    font-size:12px;\r\n    color:#226aff;\r\n    display:flex;\r\n    justify-content: space-between;\r\n}\r\n</style>\r\n\r\n"]}]}